 #cloning an public repo and uploading as private
 13  git config --global user.name "gopal das"
   14  git config --global user.email "gopal1409@gmail.com"
   15  git clone https://github.com/gopal1409/springboot-chat-app.git
   17  cd springboot-chat-app/
   18  rm -rf .git
   19  git init
   20  git add .
   22  git commit -m "my spring app"
   23  git log

###how to generate ssh key and get the public key
 25  ssh-keygen -t rsa
   26  cat /root/.ssh/id_rsa.pub
   ###aDD REMOTE REPO AND PUSH IT
    28  git remote add origin git@github.com:gopal1409/ibm-chat-app.git
   29  git push -u origin master

