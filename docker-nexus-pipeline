pipeline {
    agent any
    stages {
        stage('code scm') {
            steps {
                git credentialsId: 'git-token', url: 'git@github.com:gopal1409/ibmchatapp.git'
            }
        }
        stage('mvn package') {
            steps {
                sh 'mvn -B -DskipTests clean package'
            }
        }
    }
}
